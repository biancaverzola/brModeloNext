<mxStylesheet>
	<add as="defaultVertex">
		<add as="shape" value="label" />
		<add as="perimeter" value="rectanglePerimeter" />
		<add as="fontSize" value="11" />
		<add as="align" value="center" />
		<add as="verticalAlign" value="middle" />
		<add as="strokeColor" value="black" />
		<add as="fillColor" value="white" />
	</add>
	<add as="defaultEdge">
		<add as="shape" value="connector" />
		<add as="labelBackgroundColor" value="white" />
		<add as="endArrow" value="none" />
		<add as="fontSize" value="10" />
		<add as="align" value="center" />
		<add as="verticalAlign" value="bottom" />
		<add as="strokeColor" value="black" />
	</add>
	<add as="entity" extend="defaultVertex">
		<add as="shape" value="rectangle" />
	</add> 	
	<add as="relation" extend="defaultVertex">
		<add as="shape" value="rhombus" />
		<add as="perimeter" value="rhombusPerimeter" />
	</add>
	<add as="attribute" extend="defaultVertex">
		<add as="shape" value="attribute" />
		<add as="perimeter" value="ellipsePerimeter" />
		<add as="verticalLabelPosition" value="top" />
		<add as="verticalAlign" value="bottom" />
		<add as="labelPosition" value="right" />	
	</add>
	<add as="identifierAttribute" extend="defaultVertex">
		<add as="shape" value="identifierAttribute" />
		<add as="perimeter" value="ellipsePerimeter" />
		<add as="verticalLabelPosition" value="top" />
		<add as="verticalAlign" value="bottom" />
		<add as="labelPosition" value="right" />
		<add as="fillColor" value="black" />
	</add>
	<add as="inheritance" extend="defaultVertex">
		<add as="shape" value="triangle" ></add>
		<add as="perimeter" value="trianglePerimeter" />
		<add as="noLabel" value="1" />
	</add>	
	<add as="partialInheritance" extend="defaultVertex">
		<add as="shape" value="triangle" />
		<add as="perimeter" value="trianglePerimeter" />
		<add as="labelPosition" value="right" />
		<add as="align" value="left" />
		<add as="fontSize" value="14" />
		<add as="fontStyle" value="3" />
	</add>	
	<add as="associativeEntity" extend="defaultVertex">
		<add as="shape" value="rectangle" />
		<add as="perimeter" value="rectanglePerimeter" />
		<add as="verticalAlign" value="top" />
		<add as="align" value="right" />
	</add>
	<add as="associativeRelation" extend="defaultVertex">
		<add as="shape" value="rhombus" />
		<add as="perimeter" value="rhombusPerimeter" />
	</add>
	<add as="table" extend="defaultVertex">
		<add as="shape" value="swimlane" />
		<add as="fontSize" value="12" />
		<add as="fontStyle" value="1" />
		<add as="startSize" value="25" />
	</add>
	<add as="column" extend="defaultVertex">
		<add as="shape" value="column" />
		<add as="align" value="left" />
		<add as="spacingLeft" value="30" />
		<add as="fillColor" value="white" />
	</add>
	<add as="primaryKey" extend="column">
		<add as="image" value="/ufsc/sisinf/brmodelo2all/ui/images/chave_primaria.png" />
		<add as="imageWidth" value="15" />
		<add as="imageHeight" value="15" />
		<add as="imageAlign" value="left" />
		<add as="imageVerticalAlign" value="middle" />
	</add>
	<add as="foreignKey" extend="column">
		<add as="image" value="/ufsc/sisinf/brmodelo2all/ui/images/chave_estrangeira.png" />
		<add as="imageWidth" value="15" />
		<add as="imageHeight" value="15" />
		<add as="imageAlign" value="left" />
		<add as="imageVerticalAlign" value="middle" />
	</add>
	<add as="bothKeys" extend="column">
		<add as="image" value="/ufsc/sisinf/brmodelo2all/ui/images/ambas_chaves.png" />
		<add as="imageWidth" value="30" />
		<add as="imageHeight" value="15" />
		<add as="imageAlign" value="left" />
		<add as="imageVerticalAlign" value="middle" />
	</add>
	<add as="tableRelation" extend="relation">
		<add as="shape" value="rhombus" />
		<add as="perimeter" value="rhombusPerimeter" />
		<add as="noLabel" value="1" />
	</add>
	<add as="entityRelationConnector" extend="defaultEdge">
		<add as="edgeStyle" value="entityRelationEdgeStyle" />
	</add>
	<add as="weakEntity" extend="entityRelationConnector">
		<add as="strokeWidth" value="3" />
	</add>
	<add as="vertical" extend="defaultEdge">
		<add as="elbow" value="vertical" />
	</add>
	<add as="straight" extend="defaultVertex">
		<add as="shape" value="connector" />
		<add as="endArrow" value="classic" />
		<add as="edgeStyle" value="none" />
	</add>
	<add as="arrow" extend="defaultVertex">
		<add as="shape" value="arrow" />
		<add as="fillColor" value="red" />
	</add>
	<add as="swimlane" extend="defaultVertex">
		<add as="shape" value="swimlane" />
		<add as="fontSize" value="12" />
		<add as="fontStyle" value="1" />
		<add as="startSize" value="23" />
	</add>
	<add as="group" extend="defaultVertex">
		<add as="verticalAlign" value="top" />
		<add as="dashed" value="1" />
		<add as="opacity" value="50" />
	</add>
	<add as="ellipse" extend="defaultVertex">
		<add as="shape" value="ellipse" />
		<add as="perimeter" value="ellipsePerimeter" />
	</add>
	<add as="rhombus" extend="defaultVertex">
		<add as="shape" value="rhombus" />
		<add as="perimeter" value="rhombusPerimeter" />
	</add>
	<add as="triangle" extend="defaultVertex">
		<add as="shape" value="triangle" />
		<add as="perimeter" value="trianglePerimeter" />
	</add>
	<add as="line" extend="defaultVertex">
		<add as="shape" value="line" />
		<add as="strokeWidth" value="4" />
		<add as="labelBackgroundColor" value="white" />
		<add as="verticalAlign" value="top" />
		<add as="spacingTop" value="8" />
	</add>
	<add as="image" extend="defaultVertex">
		<add as="shape" value="image" />
		<add as="labelBackgroundColor" value="white" />
		<add as="verticalLabelPosition" value="bottom" />
		<add as="verticalAlign" value="top" />
	</add>
	<add as="roundImage" extend="image">
		<add as="perimeter" value="ellipsePerimeter" />
	</add>
	<add as="rhombusImage" extend="image">
		<add as="perimeter" value="rhombusPerimeter" />
	</add>
</mxStylesheet>